load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "go_default_library",
    srcs = [
        "doc.go",
        "event.go",
    ],
    importpath = "golang.org/x/tools/internal/event",
    visibility = ["//:__subpackages__"],
    deps = [
        "//internal/event/core:go_default_library",
        "//internal/event/keys:go_default_library",
        "//internal/event/label:go_default_library",
    ],
)

go_test(
    name = "go_default_test",
    srcs = ["bench_test.go"],
    embed = [":go_default_library"],
    deps = [
        "//internal/event/core:go_default_library",
        "//internal/event/export:go_default_library",
        "//internal/event/keys:go_default_library",
        "//internal/event/label:go_default_library",
    ],
)
